// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model matkul {
  id     Int    @id @default(autoincrement())
  kode   String
  nama   String
}

model prodi {
  id     Int    @id @default(autoincrement())
  kode   String
  nama   String
  kepala String
}